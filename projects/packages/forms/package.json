{
	"private": true,
	"name": "@automattic/jetpack-forms",
	"version": "2.0.1",
	"description": "Jetpack Forms",
	"homepage": "https://github.com/Automattic/jetpack/tree/HEAD/projects/packages/forms/#readme",
	"bugs": {
		"url": "https://github.com/Automattic/jetpack/labels/[Package] Forms"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Automattic/jetpack.git",
		"directory": "projects/packages/forms"
	},
	"license": "GPL-2.0-or-later",
	"author": "Automattic",
	"scripts": {
		"build": "pnpm run clean && pnpm run build:blocks && pnpm run build:contact-form && pnpm run build:dashboard && pnpm run module:build",
		"build-production": "NODE_ENV=production BABEL_ENV=production pnpm run build && pnpm run validate",
		"build:blocks": "webpack --config ./tools/webpack.config.blocks.js",
		"build:contact-form": "webpack --config ./tools/webpack.config.contact-form.js",
		"build:dashboard": "webpack --config ./tools/webpack.config.dashboard.js",
		"clean": "rm -rf dist/ .cache/",
		"test": "jest --config=tests/jest.config.js",
		"test:contact-form": "jest --testPathPatterns=tests/js/contact-form",
		"test-coverage": "pnpm run test --coverage",
		"typecheck": "tsc --noEmit",
		"validate": "pnpm exec validate-es --no-error-on-unmatched-pattern dist/",
		"watch": "concurrently 'pnpm:build:blocks --watch' 'pnpm:build:contact-form --watch' 'pnpm:build:dashboard --watch' 'pnpm:module:watch'",
		"module:build": "webpack --config ./tools/webpack.config.modules.js",
		"module:watch": "webpack --watch --config ./tools/webpack.config.modules.js"
	},
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"dependencies": {
		"@automattic/babel-plugin-replace-textdomain": "workspace:*",
		"@automattic/jetpack-analytics": "workspace:*",
		"@automattic/jetpack-components": "workspace:*",
		"@automattic/jetpack-connection": "workspace:*",
		"@automattic/jetpack-script-data": "workspace:*",
		"@automattic/jetpack-shared-extension-utils": "workspace:*",
		"@wordpress/base-styles": "6.1.0",
		"@wordpress/block-editor": "14.20.0",
		"@wordpress/blocks": "14.14.0",
		"@wordpress/components": "29.11.0",
		"@wordpress/compose": "7.25.0",
		"@wordpress/core-data": "7.25.0",
		"@wordpress/data": "10.25.0",
		"@wordpress/dataviews": "4.17.0",
		"@wordpress/dependency-extraction-webpack-plugin": "6.25.0",
		"@wordpress/element": "6.25.0",
		"@wordpress/hooks": "4.25.0",
		"@wordpress/i18n": "5.25.0",
		"@wordpress/icons": "10.25.0",
		"@wordpress/interactivity": "6.25.0",
		"@wordpress/notices": "5.25.0",
		"@wordpress/primitives": "4.25.0",
		"@wordpress/url": "4.25.0",
		"clsx": "2.1.1",
		"copy-webpack-plugin": "13.0.0",
		"email-validator": "2.0.4",
		"gridicons": "3.4.2",
		"lodash": "4.17.21",
		"react-redux": "7.2.8",
		"react-router": "7.6.2",
		"react-transition-group": "^4.4.5",
		"redux": "4.2.1",
		"redux-thunk": "2.3.0",
		"semver": "7.7.1",
		"webpack": "5.94.0",
		"webpack-cli": "6.0.1"
	},
	"devDependencies": {
		"@automattic/color-studio": "4.1.0",
		"@automattic/jetpack-base-styles": "workspace:*",
		"@automattic/jetpack-webpack-config": "workspace:*",
		"@automattic/remove-asset-webpack-plugin": "workspace:*",
		"@babel/core": "7.27.4",
		"@babel/runtime": "7.27.4",
		"@testing-library/dom": "10.4.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/lodash": "4.17.17",
		"@types/react": "18.3.23",
		"@types/react-dom": "18.3.7",
		"@wordpress/api-fetch": "7.25.0",
		"@wordpress/babel-plugin-import-jsx-pragma": "5.25.0",
		"@wordpress/browserslist-config": "6.25.0",
		"@wordpress/date": "5.25.0",
		"autoprefixer": "10.4.20",
		"concurrently": "7.6.0",
		"glob": "11.0.2",
		"jest": "30.0.0",
		"jest-environment-jsdom": "30.0.0",
		"jquery": "3.6.0",
		"postcss": "8.4.47",
		"postcss-loader": "8.1.1",
		"sass-embedded": "1.87.0",
		"sass-loader": "16.0.5",
		"typescript": "5.8.3"
	},
	"optionalDependencies": {
		"react": "18.3.1",
		"react-dom": "18.3.1"
	}
}
